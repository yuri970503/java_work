
public class identifier {

	public static void main(String[] args) {
		
		//꼭 지켜야 할 규칙 (안 지킬 시 에러가 발생할 수 있습니다.)
		// 1. 식별자의 이름은 중복을 허용하지 않습니다.      클래스나 함수도 같음
		//그리고 대/소문자를 철저하게 구분합니다.
		
		int age = 35;
//		int age = 40; (x)
		int Age = 40;
		System.out.println(age);
		System.out.println(Age);
		
		
		
		
		
		
	}

}
